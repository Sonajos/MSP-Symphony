{
  "name": "symphony-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl",
    "build": "ng build",
    "prod-build": "ng build --prod",
    "test": "ng test --codeCoverage=true",
    "test-headless": "ng test --watch=false --codeCoverage=true --browsers=ChromiumHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier \"src/**/*.{js,json,ts,html,css,less}\" --write",
    "lint:ts": "tslint -p tsconfig.json",
    "lint:ts-autofix": "tslint --fix -p tsconfig.json",
    "precommit": "lint-staged",
    "prepush": "npm run lint:ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-cert": "openssl req -newkey rsa:2048 -x509 -nodes -keyout ssl/server.key -new -out ssl/server.crt -config ssl/openssl-custom.cnf -sha256 -days 365"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@ngrx/effects": "^9.2.1",
    "@ngrx/store": "^9.2.1",
    "@ngrx/store-devtools": "^9.2.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/proj4": "^2.5.0",
    "core-js": "^2.5.4",
    "d3": "^5.15.0",
    "d3-sankey": "^0.12.3",
    "hav-components": "^1.3.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "ol": "^6.3.1",
    "proj4": "^2.6.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@babel/core": "^7.3.4",
    "@storybook/addon-a11y": "~5.3.12",
    "@storybook/addon-actions": "~5.3.12",
    "@storybook/addon-knobs": "~5.3.12",
    "@storybook/addon-links": "~5.3.12",
    "@storybook/addon-notes": "~5.3.12",
    "@storybook/addon-storysource": "^5.3.12",
    "@storybook/addons": "~5.3.12",
    "@storybook/angular": "~5.3.12",
    "@types/d3": "^5.7.2",
    "@types/d3-sankey": "^0.11.0",
    "@types/geojson": "^7946.0.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.11.1",
    "@types/ol": "^6.3.1",
    "@types/uuid": "^3.4.6",
    "angular-in-memory-web-api": "^0.9.0",
    "autoprefixer": "^8.1.0",
    "babel-loader": "^8.0.5",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "protractor": "~5.4.0",
    "raw-loader": "^1.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5"
  },
    "peerDependencies": {
      "@angular/cdk": "^9.1.0"
    },
    "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run prepush"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,html,css,less}": [
      "prettier --write",
      "git add"
    ]
  }
}
